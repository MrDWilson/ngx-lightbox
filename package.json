{
  "name": "ngx-lightbox",
  "version": "3.0.0",
  "description": "A port >= angular5 for lightbox2",
  "main": "index.js",
  "dependencies": {
    "@types/node": "^20.8.2",
    "file-saver": "^2.0.5",
    "ngx-filesaver": "^15.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.0",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "^15.0.0",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/compiler-cli": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@types/file-saver": "^2.0.5",
    "@types/jasmine": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "copyfiles": "^2.4.1",
    "core-js": "^3.33.0",
    "del-cli": "^5.1.0",
    "eslint": "^8.33.0",
    "jasmine-core": "^5.1.1",
    "karma": "^6.4.2",
    "karma-jasmine": "^5.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.4.0",
    "karma-webpack": "^5.0.0",
    "ngx-lightbox": "^3.0.0",
    "phantomjs-prebuilt": "^2.1.16",
    "rxjs": "^7.8.1",
    "typescript": "^4",
    "webpack": "^5.88.2",
    "zone.js": "^0.13.0"
  },
  "scripts": {
    "start": "./node_modules/.bin/ng serve",
    "build": "./node_modules/.bin/ng build",
    "test": " ./node_modules/.bin/ng lint && ./node_modules/.bin/ng test",
    "lint": "./node_modules/.bin/ng lint",
    "prepublishOnly": "./node_modules/.bin/ngc && ./node_modules/.bin/copyfiles -u 1 src/img/* src/*.js src/*.d.ts src/*.js.map src/*.metadata.json src/*.css .",
    "postpublish": "./node_modules/.bin/del '*.{service,component,module,spec,ngfactory}.js' '*.{metadata,ngsummary}.json' index.js webpack.config.js  '*.{ts,css,map}' '{compiled,img}' 'src/*.{d.ts,ngfactory.ts,js,map,json}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/themyth92/ngx-lightbox.git"
  },
  "keywords": [
    "lightbox2",
    "angularjs",
    "directives",
    "lightbox2",
    "angular",
    "directives"
  ],
  "author": "themyth92",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/themyth92/ngx-lightbox/issues"
  },
  "homepage": "https://github.com/themyth92/ngx-lightbox#readme"
}
